# Path to the binary
binary:dipjudge.py

# Interpreter, if any
interpreter:python

checkout_location:.
default_checkout:CodeNoFailures

config_module:default

log_file:output

# Run-dependent text
output:Running Diplomacy Judge

# Need this at Carmen
display_module:carmen

# Write locally to avoid collisions
write_tmp_files:TargetApp

# Tell it we can't save this version
unsaveable_version:unsaveable

self_test_use_collection:true
