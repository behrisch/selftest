# Path to the binary
binary:${TEXTTEST_CHECKOUT}/dipjudge.py

default_checkout:CodeNoFailures

# Tell it we can't save this version
unsaveable_version:unsaveable

collect_standard_error:0

# Make sure everything is portable
text_diff_program:ndiff
diff_program:tkdiff

[run_dependent_text]
output:Running Diplomacy Judge
[end]

test_list_files_directory:.
