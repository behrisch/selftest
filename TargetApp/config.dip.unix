config_module:unixConfig

# Don't use standard error, dipjudge doesn't write it anyway
# and we want tests to work on windows
collect_standard_error:0

# Use collection for self tests, so we can compare reports
[batch_use_collection]
default:true

[batch_version]
default:unix
self_test:interr

# Add a recognised section with no end marker last, to catch config file bug...
[gui_entry_overrides]
add_environment_file:0
