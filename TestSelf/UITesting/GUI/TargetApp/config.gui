# Path to the executable.
executable:$TEXTTEST_CHECKOUT/videostore.py

interpreter:ttpython

checkout_location:$TEXTTEST_HOME
default_checkout:source_with_pyc

# Might as well have nice printouts
full_name:VideoStore

use_case_record_mode:GUI

# Force regeneration
home_operating_system:posix

# No need to involve this functionality 
# It doesn't work on Windows and the outer
# layer will fix things for us anyway
virtual_display_machine:{CLEAR LIST}
