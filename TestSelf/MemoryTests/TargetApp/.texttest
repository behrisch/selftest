# Plugin programs for TextTest GUI
view_program:xemacs
diff_program:tkdiff
follow_program:tail -f

# Colours for TextTest GUI. These are used in the left window for tests
# Valid colour values can be found from the last column of the file
# /usr/lib/python2.2/site-packages/pynche/X/rgb.txt
[test_colours]
success:pale green
failure:orange
running:turquoise
not_started:purple
static:pale green
app_static:purple

# Colours for TextTest GUI. These are used in the right window for files
[file_colours]
success:green
failure:red
running:yellow
not_started:white
static:pale green
app_static:purple
