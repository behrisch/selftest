# Path to the binary
binary:${TEXTTEST_CHECKOUT}/source/writediags.py

collect_standard_error:0

# Force regeneration of files
home_operating_system:posix

text_diff_program:ndiff

[diagnostics]
trace_level_variable:LOG_LEVEL
input_directory_variable:TESTDIAG_READDIR
write_directory_variable:TESTDIAG_WRITEDIR
configuration_file:log4py.conf
[end]
