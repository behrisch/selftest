
# Path to the executable
executable:${TEXTTEST_HOME}/sleep.py

log_file:output

home_operating_system:posix

config_module:queuesystem

default_interface:console

# Copied from 'standard'
suppress_stderr_text:RANDR

[run_dependent_text]
errors*:RANDR
[end]

[batch_use_collection]
default:true

[run_dependent_text]
errors:locale

[version_priority]
extra_test:1
